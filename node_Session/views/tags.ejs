<!-- タグ一覧ページ　 -->
<!DOCTYPE html>
<html>

<head>
    <title>

    </title>
<%- include('./component/in_css.ejs') %>
</head>

<body>
    <!-- Page Layout here -->
    <div class="row">

      <div class="col s3 m3 l3">
        <!-- Grey navigation panel -->
        <%- include('./component/sidebar.ejs') %>
      </div>  
      <div class="col s12 m7 l7 main">
      <h1 class="page-title"><%= title %></h1>
      <p><%= msg %></p>
        <!-- Teal page content  -->
       
          <div class="card row orange darken-3">
            <div class="card-content white-text">
              <button id="addTag_btn" class="btn btn-floating waves-effect waves-light right  deep-orange darken-3"><i class="material-icons">add</i></button>
              <span class="card-title">Tags</span>
              <% tagList.forEach(function(tag) {  %>
              
              <div id="<%= tag.id %>" value="<%= tag.value.rev%>" class="chip orange lighten-4"><%= tag.value.title %></div>
              
              <% }); %>
            </div>
          </div>
      </div>

    </div>
</body>
 <%- include('./component/in_js.ejs') %>
</html>